<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/permission_layout_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_transparency_700">

    <ViewStub
        android:id="@+id/stub_import"
        android:inflatedId="@id/stub_import"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/btn_setup"
        android:layout_width="180dp"
        android:layout_height="46dp"
        android:layout_below="@id/stub_import"
        android:layout_centerHorizontal="true"
        android:text="@string/permission_layout_setip"
        android:textSize="@dimen/basic_text_size_5"
        android:textColor="@color/white"
        android:gravity="center"
        android:layout_marginTop= "48dp"
        android:background="@drawable/dialog_button_blue_bg" />


</RelativeLayout>